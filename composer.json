{
	"name": "serato/slimulator",
	"homepage": "https://github.com/serato/slimulator",
	"description": "A mock request tool for web applications using the Slim PHP web framework",
	"type": "library",
	"require": {
		"php": "^8.3",
		"slim/slim": "^4.13.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.3",
		"squizlabs/php_codesniffer": "~2.0",
		"aws/aws-sdk-php": "^3.0",
		"phpstan/phpstan": "^1.10.0",
		"enlightn/security-checker": "^1.4",
		"rector/rector": "^0.18.0"
	},
	"autoload": {
		"psr-4": {
			"Serato\\Slimulator\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\Slimulator\\Test\\": "tests/"
		}
	}
}

{
	"name": "serato/slimulator",
	"homepage": "https://github.com/serato/slimulator",
	"description": "A mock request tool for web applications using the Slim PHP web framework",
	"type": "library",
	"require": {
		"php" : "^7.1",
		"slim/slim": "^3.5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.0",
		"squizlabs/php_codesniffer": "~2.0",
		"aws/aws-sdk-php": "^3.0",
		"phpstan/phpstan": "^0.11.0",
		"phpstan/phpstan-phpunit": "^0.11.0",
		"apigen/apigen": "dev-master#85290e8",
		"roave/better-reflection": "3.3.0 as dev-master",
		"sensiolabs/security-checker": "^6.0"
	},
	"autoload": {
		"psr-4": {
			"Serato\\Slimulator\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\Slimulator\\Test\\": "tests/"
		}
	}
}

{
	"name": "serato/slimulator",
	"homepage": "https://github.com/serato/slimulator",
	"description": "A mock request tool for web applications using the Slim PHP web framework",
	"type": "library",
	"require": {
		"php": "^7.1 || ^8.2",
		"slim/slim": "^3.5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7 || ^8",
		"squizlabs/php_codesniffer": "^3",
		"aws/aws-sdk-php": "^3.0",
		"phpstan/phpstan": "^1.4",
		"enlightn/security-checker": "^1.4 || ^2"
	},
	"autoload": {
		"psr-4": {
			"Serato\\Slimulator\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\Slimulator\\Test\\": "tests/"
		}
	}
}
